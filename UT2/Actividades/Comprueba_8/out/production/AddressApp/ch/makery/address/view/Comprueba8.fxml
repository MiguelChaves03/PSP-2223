            fx:controller="ch.makery.address.controller.Comprueba8">
    <children>
        <Button fx:id="btnComenzar" layoutX="220.0" layoutY="35.0" mnemonicParsing="false" onAction="#comenzar"
                prefHeight="35.0" prefWidth="175.0" text="COMENZAR PROCESO"/>
        <Button fx:id="btnReanudar1" layoutX="85.0" layoutY="100.0" mnemonicParsing="false" onAction="#reanudar"
                prefHeight="35.0" prefWidth="175.0" text="REANUDAR"/>
        <Button fx:id="btnReanudar2" layoutX="355.0" layoutY="100.0" mnemonicParsing="false" onAction="#reanudar"
                prefHeight="35.0" prefWidth="175.0" text="REANUDAR"/>
        <Button fx:id="btnSuspender1" layoutX="85.0" layoutY="165.0" mnemonicParsing="false" onAction="#suspender"
